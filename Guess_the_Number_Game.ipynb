      "source": [
        "from random import randrange\n",
        "n = randrange(1000)\n",
        "while True:\n",
        "  v = int(input())\n",
        "  if n == v:\n",
        "    print('You Win!')\n",
        "    break\n",
        "  print('Smaller' if (n < v) else 'Bigger')"
      ],
